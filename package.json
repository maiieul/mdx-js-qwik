{
  "name": "maiieul-mdx-js-qwik",
  "version": "0.1.2",
  "description": "MDX custom components with Qwik backend",
  "author": "langbamit",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maiieul/mdx-js-qwik.git"
  },
  "bugs": {
    "url": "https://github.com/maiieul/mdx-js-qwik/issues"
  },
  "homepage": "https://github.com/maiieul/mdx-js-qwik/tree/main/#readme",
  "keywords": [
    "qwik",
    "qwik-city",
    "mdx",
    "mdxjs"
  ],
  "license": "MIT",
  "main": "./lib/index.qwik.mjs",
  "qwik": "./lib/index.qwik.mjs",
  "types": "./lib-types/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.qwik.mjs",
      "require": "./lib/index.qwik.cjs",
      "types": "./lib-types/index.d.ts"
    }
  },
  "files": [
    "lib",
    "lib-types"
  ],
  "engines": {
    "node": ">=15.0.0"
  },
  "private": false,
  "type": "module",

  "scripts": {
    "build": "qwik build",
    "build.lib": "vite build --mode lib",
    "build.types": "tsc --emitDeclarationOnly",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "link.dist": "cd /lib && pnpm link --global",
    "release": "np",
    "start": "vite --open --mode ssr",
    "test": "echo \"No test specified\" && exit 0",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@builder.io/qwik": "0.15.2",
    "@mdx-js/mdx": "^2.1.5",
    "@types/eslint": "8.4.10",
    "@types/node": "latest",
    "@typescript-eslint/eslint-plugin": "5.43.0",
    "@typescript-eslint/parser": "5.43.0",
    "eslint": "8.28.0",
    "eslint-plugin-qwik": "0.15.2",
    "node-fetch": "3.3.0",
    "np": "7.6.1",
    "prettier": "2.7.1",
    "typescript": "4.9.3",
    "vite": "^4.0.1",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "peerDependencies": {
    "@builder.io/qwik": "^0.14.0 || ^0.15.0"
  },
  "dependencies": {
    "@types/estree-jsx": "^1.0.0",
    "@types/mdx": "^2.0.3",
    "unified": "^10.0.0"
  }
}
